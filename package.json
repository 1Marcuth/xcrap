{
  "name": "xcrap",
  "version": "1.10.7",
  "description": "Xcrap is a Web Scraping framework for JavaScript, designed to facilitate the process of extracting data from multiple pages or even just one, with a sophisticated page parsing system.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "dev": "ts-node src/index.ts",
    "generate-exports": "node dev-scripts/generate-exports.cjs"
  },
  "keywords": [],
  "author": "Marcuth",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "node-html-parser": "^6.1.13",
    "puppeteer": "^23.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/fake-useragent": "^1.0.0",
    "@types/node": "^20.16.5",
    "fake-useragent": "^1.0.1",
    "microbundle": "^0.15.1",
    "rimraf": "^6.0.1",
    "rollup": "^2.79.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "publishConfig": {
    "source": "./src/index.ts",
    "main": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./clients/axios.client": {
      "import": "./dist/clients/axios.client.mjs",
      "require": "./dist/clients/axios.client.cjs",
      "types": "./dist/clients/axios.client.d.ts"
    },
    "./clients/base.client": {
      "import": "./dist/clients/base.client.mjs",
      "require": "./dist/clients/base.client.cjs",
      "types": "./dist/clients/base.client.d.ts"
    },
    "./clients": {
      "import": "./dist/clients/index.mjs",
      "require": "./dist/clients/index.cjs",
      "types": "./dist/clients/index.d.ts"
    },
    "./clients/puppeteer.client": {
      "import": "./dist/clients/puppeteer.client.mjs",
      "require": "./dist/clients/puppeteer.client.cjs",
      "types": "./dist/clients/puppeteer.client.d.ts"
    },
    "./common/types": {
      "import": "./dist/common/types.mjs",
      "require": "./dist/common/types.cjs",
      "types": "./dist/common/types.d.ts"
    },
    "./parsing/extractors.parsing": {
      "import": "./dist/parsing/extractors.parsing.mjs",
      "require": "./dist/parsing/extractors.parsing.cjs",
      "types": "./dist/parsing/extractors.parsing.d.ts"
    },
    "./parsing": {
      "import": "./dist/parsing/index.mjs",
      "require": "./dist/parsing/index.cjs",
      "types": "./dist/parsing/index.d.ts"
    },
    "./parsing/page-set.parsing": {
      "import": "./dist/parsing/page-set.parsing.mjs",
      "require": "./dist/parsing/page-set.parsing.cjs",
      "types": "./dist/parsing/page-set.parsing.d.ts"
    },
    "./parsing/page.parsing": {
      "import": "./dist/parsing/page.parsing.mjs",
      "require": "./dist/parsing/page.parsing.cjs",
      "types": "./dist/parsing/page.parsing.d.ts"
    },
    "./transforming/data.transforming": {
      "import": "./dist/transforming/data.transforming.mjs",
      "require": "./dist/transforming/data.transforming.cjs",
      "types": "./dist/transforming/data.transforming.d.ts"
    },
    "./transforming": {
      "import": "./dist/transforming/index.mjs",
      "require": "./dist/transforming/index.cjs",
      "types": "./dist/transforming/index.d.ts"
    },
    "./transforming/transformers.transformation": {
      "import": "./dist/transforming/transformers.transformation.mjs",
      "require": "./dist/transforming/transformers.transformation.cjs",
      "types": "./dist/transforming/transformers.transformation.d.ts"
    },
    "./transforming/utils.transformation": {
      "import": "./dist/transforming/utils.transformation.mjs",
      "require": "./dist/transforming/utils.transformation.cjs",
      "types": "./dist/transforming/utils.transformation.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs",
      "types": "./dist/utils/index.d.ts"
    },
    "./utils/url.util": {
      "import": "./dist/utils/url.util.mjs",
      "require": "./dist/utils/url.util.cjs",
      "types": "./dist/utils/url.util.d.ts"
    },
    "./xcrap": {
      "import": "./dist/xcrap.mjs",
      "require": "./dist/xcrap.cjs",
      "types": "./dist/xcrap.d.ts"
    }
  }
}